{
  "manifest_version": 2,

  "name": "Open tabs",
  "description": "This extension will open tabs, based on the url you specify",
  "version": "1.0",

  "browser_action": {
    "default_icon" : "icon.png",
    "default_popup": "popup.html",
    "default_title": "LEGOO"
  },
  "background":{
    "scripts": ["popup.js"]
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}